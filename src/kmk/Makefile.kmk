	HAVE_CONFIG_H \
	$(TEMPLATE_BIN_DEFS) \
	KBUILD_SVN_REV=$(KBUILD_SVN_REV) \
	KBUILD_TYPE=$(TMP_QUOTE_SLASH)"$(KBUILD_TYPE)$(TMP_QUOTE_SLASH)"
