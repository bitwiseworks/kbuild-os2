	$(kmk_0_OUTDIR)/sysexits.h \
	$(kmk_0_OUTDIR)/unistd.h \
	$(kmk_0_OUTDIR)/paths.h \
	$(kmk_0_OUTDIR)/grp.h \
	$(kmk_0_OUTDIR)/pwd.h \
	$(kmk_0_OUTDIR)/inttypes.h
