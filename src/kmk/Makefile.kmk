TEMPLATE_BIN-KMK_DEFS += KBUILD_SVN_GIT=$(KBUILD_SVN_GIT)
endif
TEMPLATE_BIN-KMK_DEPS = \
	$(kmk_0_OUTDIR)/config.h \
	$(kmk_0_OUTDIR)/fts.h
TEMPLATE_BIN-KMK_CLEAN = $(TEMPLATE_BIN-KMK_DEPS)
TEMPLATE_BIN-KMK_DEPS.solaris = \
	$(kmk_0_OUTDIR)/paths.h
